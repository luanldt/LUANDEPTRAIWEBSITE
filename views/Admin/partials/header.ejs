        <div class="form-group">
            <input type="text" class="form-control" placeholder="Search">
        </div>
    </form>
    <ul class="nav menu">
        <li class="<%= active == 'Dashboard' ? 'active' : '' %>">
            <a href="/Admin/">
                <svg class="glyph stroked dashboard-dial">
                    <use xlink:href="#stroked-dashboard-dial"></use>
                </svg>
                Dashboard</a></li>
        <li class="<%= active == 'Projects' ? 'active' : '' %>"><a href="/Admin/Projects">
                <svg class="glyph stroked folder">
                    <use xlink:href="#stroked-folder"/>
                </svg>
                Projects</a></li>
        <li class="<%= active == 'CV' ? 'active' : '' %>"><a href="charts.html">
                <svg class="glyph stroked clipboard with paper">
                    <use xlink:href="#stroked-clipboard-with-paper"/>
                </svg>
                CV</a></li>
        <li class="parent <%= active == 'Websites' ? 'active' : '' %>" >
            <a href="#">
				<span data-toggle="collapse" href="#sub-item-1"><svg class="glyph stroked app window with content"><use
                                xlink:href="#stroked-app-window-with-content"/></svg> Websites</span>
            </a>
            <ul class="children collapse" id="sub-item-1">
                <li>
                    <a class="" href="#">
                        <svg class="glyph stroked chevron-right">
                            <use xlink:href="#stroked-chevron-right"></use>
                        </svg>
                        Create
                    </a>
                </li>
                <li>
                    <a class="" href="#">
                        <svg class="glyph stroked chevron-right">
                            <use xlink:href="#stroked-chevron-right"></use>
                        </svg>
                        Manager
                    </a>
                </li>
            </ul>
        </li>
        <li class="parent <%= active == 'Account' ? 'active' : '' %>" >
            <a href="#">
				<span data-toggle="collapse" href="#sub-item-2"><svg class="glyph stroked male user "><use
                                xlink:href="#stroked-male-user"/></svg> Account</span>
            </a>
            <ul class="children collapse" id="sub-item-2">
                <li class="<%= active == 'Account' ? 'active' : '' %>">
                    <a class="" href="/Admin/Account/">
                        <svg class="glyph stroked chevron-right">
